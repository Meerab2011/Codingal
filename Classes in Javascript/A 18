<html>
    <head>

    </head>
    <body>
        <script>
            class student{
                constructor(name,year){
this.n=name;
this.y=year;
                }
                age(){
                    var d=new Date();
                    return d.getFullYear()-this.y;//2024-2008
                }

            }
            class gr extends students{
                constructor(name, year, grade){
                    super(name,year);
                    this.g=grade;
                }
                result(){
                    return "My name is " + this.n + ", I born in " + this.y + ", and my age is " + this.age()+", I secured"+ this.g;
                }
            }
            s1=new student("Noureen",2008);
            s2 = new student("Meerab",2011);
            document.write("My name is "+s1.n+", I born in "+s1.y+", and my age is "+s1.age()+"<br>")
            document.write("My name is " + s2.n + ", I born in " + s2.y + ", and my age is " + s2.age() + "<br>")
g1=new gr("Devanshu",2011,"A+");
g1.result();
        </script>
    </body>
</html>
