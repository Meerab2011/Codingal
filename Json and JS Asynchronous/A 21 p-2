</head>
<body>
    <h1>Callback</h1>
    <p id="i1"></p>
    <script>
        function disp(value){
document.getElementById("i1").innerHTML=value;

        }
        function calc(n1,n2,fun){
            let sum=n1+n2;
            fun (sum);

        }
        calc (7,4,disp);
    </script>
</body>
</html>